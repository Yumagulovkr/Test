local letters = {'a','b','c','d','e','f','g','h','i','j','k','l','m','n','o','p','q','r','s','t','u','v','w','x','y','z','1','2','3','4','5','6','7','8','9'}
local characteramount = 32
local pending = {}

for i = 1,characteramount do
table.insert(pending, letters[math.random(1,#letters)])
end

local loading = {}
for i = math.random(1,characteramount),math.random(1,characteramount) do
pending[i] = string.upper(pending[1])
end

print(table.concat(pending, ""))
